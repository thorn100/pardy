  },
  "main": "index.js",
  "files": [
    "*.js",
    "dist"
  ],
  "dependencies": {
    "deduce": "^0.1.3",
    "document-register-element": "^0.5.4",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "localenv": "^0.2.2",
    "morphdom": "^2.0.1",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "reset-css": "^2.0.2011012603",
    "socket.io": "^1.4.8",
    "tcomb": "^3.2.9"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "localenvify": "^1.0.1",
    "nodemon": "^1.10.0",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^8.1.2"
  },
  "scripts": {
    "start": "node src",
    "build": "npm run build:html && npm run build:css && npm run build:js",
    "build:html": "cp src/client/index.html dist/client/index.html",
    "build:css": "postcss -u postcss-import -o dist/client/assets/css/index.css src/client/assets/css/index.css",
    "build:js": "browserify -t localenvify -o dist/client/assets/js/index.js src/client/assets/js/index.js"
  },
  "engines": {
    "node": ">= 6.3.1"
  },
  "browser": {
    "ws": false
  }
}
